"*":
  "atom-beautify":
    general:
      _analyticsUserId: "6c76b40e-a881-4b14-aa78-31401c1ca37c"
  "atom-notes":
    directory: "/home/sergej/Dropbox/notes"
  "autocomplete-python":
    pythonPaths: "/home/sergej/.virtualenvs/$PROJECT_NAME/bin/python;/usr/bin/python3"
    useKite: false
  "better-git-blame":
    searchInLayerEnabled: false
  core:
    autoHideMenuBar: true
    packagesWithKeymapsDisabled: [
      "atom-beautify"
      "split-diff"
    ]
    projectHome: "/home/sergej/Code"
    restorePreviousWindowsOnStart: "no"
    telemetryConsent: "limited"
    themes: [
      "one-light-ui"
      "one-light-syntax"
    ]
    useTreeSitterParsers: true
  "django-templates":
    defaultToDjangoTemplatesForFilePathsContaining: [
      "templates"
    ]
  editor:
    atomicSoftTabs: false
    fontFamily: "Fira Mono"
    fontSize: 13
    invisibles: {}
    scrollPastEnd: true
    softWrap: true
    tabLength: 4
  "exception-reporting":
    userId: "6bd9eac5-fb33-596d-8947-bd2c2321b109"
  "file-icons": {}
  "gtk-dark-theme":
    enabled: false
  latex:
    buildOnSave: true
    enableExtendedBuildMode: false
    opener: "evince"
  "one-dark-ui": {}
  "one-light-ui": {}
  "open-terminal-here":
    command: "gnome-terminal"
  "package-sync":
    createOnChange: true
    forceOverwrite: true
  pigments:
    notifyReloads: false
  "project-plus":
    notifications: false
    showPath: true
  "spell-check":
    grammars: [
      "source.asciidoc"
      "source.gfm"
      "text.git-commit"
      "text.plain"
      "text.plain.null-grammar"
      "text.tex.latex"
      "text.xml"
      "text.html.basic"
    ]
    knownWords: [
      "Chodarev"
      "metamodel"
    ]
    localePaths: [
      "/usr/share/myspell/"
    ]
    locales: [
      "en-US"
      "sk-SK"
    ]
  "split-diff":
    colors: {}
  "tree-view":
    hideIgnoredNames: true
  "updater-notify":
    pkgType: "RedHat"
  welcome:
    showOnStartup: false
  whitespace:
    removeTrailingWhitespace: false
".basic.html.text":
  editor:
    tabLength: 2
".beamer.latex.tex.text":
  editor:
    tabLength: 2
".bibtex.text":
  editor:
    tabLength: 2
".html.liquid.text":
  editor:
    tabLength: 2
".latex.memoir.tex.text":
  editor:
    tabLength: 2
".latex.tex.text":
  editor:
    tabLength: 2
".text.xml":
  editor:
    tabLength: 2
".text.xml.xsl":
  editor:
    tabLength: 2
