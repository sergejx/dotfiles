partial alphanumeric_keys
xkb_symbols "sergejx" {
    name[Group1]= "Slovak (sergejx)";

    include "sk(qwerty)"

    key <AE11>  { [     equal,    percent,               endash,               emdash ] };

    key <AD10>  { [         p,          P,   doublelowquotemark,   singlelowquotemark ] };
    key <AD11>  { [    uacute,      slash,  leftdoublequotemark,  leftsinglequotemark ] };
    key <AD12>  { [adiaeresis,  parenleft, rightdoublequotemark, rightsinglequotemark ] };

    key <AC11>  { [apostrophe,     exclam,              section,              ssharp ] };

    key <SPCE>  { [     space,      space,                space,        nobreakspace ] };

    include "level3(ralt_switch)"
};

